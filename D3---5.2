library(scatterplot3d)

# Create the data frame
data <- data.frame(
  Student = c("A", "B", "C", "D", "E"),
  Math_Score = c(85, 72, 90, 78, 88),
  Science_Score = c(78, 85, 80, 75, 82),
  Attendance = c(95, 92, 98, 85, 93)
)

# Create a 3D scatter plot
scatterplot3d(
  x = data$Math_Score, 
  y = data$Science_Score, 
  z = data$Attendance,
  pch = 16,
  color = "blue",
  xlab = "Math Score",
  ylab = "Science Score",
  zlab = "Attendance (%)",
  main = "3D Scatter Plot of Math Score, Science Score, and Attendance"
)
