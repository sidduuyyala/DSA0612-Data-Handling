# Load necessary libraries
library(ggplot2)

# Create the data frame
data <- data.frame(
  Student = c("A", "B", "C", "D", "E"),
  Math_Score = c(85, 72, 90, 78, 88),
  Science_Score = c(78, 85, 80, 75, 82),
  Attendance = c(95, 92, 98, 85, 93)
)

# Scatter plot of Science Score vs. Math Score
ggplot(data, aes(x = Math_Score, y = Science_Score)) +
  geom_point() +
  geom_smooth(method = "lm", col = "blue") +
  ggtitle("Science Score vs. Math Score") +
  xlab("Math Score") +
  ylab("Science Score")

# Scatter plot of Science Score vs. Attendance
ggplot(data, aes(x = Attendance, y = Science_Score)) +
  geom_point() +
  geom_smooth(method = "lm", col = "blue") +
  ggtitle("Science Score vs. Attendance") +
  xlab("Attendance (%)") +
  ylab("Science Score")

# Fit a linear model with both predictors
model <- lm(Science_Score ~ Math_Score + Attendance, data = data)

# Display the summary of the linear model
summary(model)
