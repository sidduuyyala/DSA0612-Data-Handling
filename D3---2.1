# Load necessary libraries
library(ggplot2)

# Create the data frame
data <- data.frame(
  Date = as.Date(c("2023-01-01", "2023-01-02", "2023-01-03", "2023-01-04", "2023-01-05")),
  Stock_Price = c(100, 102, 98, 105, 108),
  Volume_Traded = c(2.5, 3.0, 2.2, 2.8, 3.5),
  Market_Cap = c(500, 510, 490, 525, 540)
)

# Plot the data
# Relationship between Stock Price and Volume Traded
ggplot(data, aes(x = Volume_Traded, y = Stock_Price)) +
  geom_point() +
  geom_smooth(method = "lm", se = FALSE) +
  ggtitle("Relationship between Stock Price and Volume Traded") +
  xlab("Volume Traded (millions)") +
  ylab("Stock Price ($)")

# Relationship between Stock Price and Market Cap
ggplot(data, aes(x = Market_Cap, y = Stock_Price)) +
  geom_point() +
  geom_smooth(method = "lm", se = FALSE) +
  ggtitle("Relationship between Stock Price and Market Cap") +
  xlab("Market Cap ($)") +
  ylab("Stock Price ($)")

# Correlation analysis
correlation_volume <- cor(data$Stock_Price, data$Volume_Traded)
correlation_market_cap <- cor(data$Stock_Price, data$Market_Cap)

print(paste("Correlation between Stock Price and Volume Traded: ", correlation_volume))
print(paste("Correlation between Stock Price and Market Cap: ", correlation_market_cap))
